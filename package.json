{
  "name": "dice-battle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "cp ./node_modules/@babylonjs/havok/lib/esm/HavokPhysics.wasm ./assets/HavokPhysics.wasm",
    "dev": "concurrently --names \" TW ,VITE\" -c \"bgBlue.bold,bgGreen.bold\" \"pnpm tailwindcss -i ./src/css/style.css -o public/style.css --watch\"  \"vite\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.45.1",
    "@babylonjs/havok": "^1.3.1",
    "@babylonjs/inspector": "^6.45.1",
    "@types/node": "^20.11.25",
    "@types/w3c-generic-sensor": "^1.0.9",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
